  "name": "yanyu-medical-ai-system",
  "version": "1.0.0",
  "description": "言语云³医疗AI智能诊疗系统 - Node.js 23.6.0优化版",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "db:backup": "cd scripts/database && ./init-database.sh --backup",
    "db:health-check": "cd scripts/database && npm run health-check",
    "db:init": "cd scripts/database && ./init-database.sh",
    "db:init:create": "cd scripts/database && ./init-database.sh --create-db",
    "db:init:schema": "cd scripts/database && ./init-database.sh --skip-data",
    "db:install-deps": "cd scripts/database && npm install",
    "db:maintenance": "cd scripts/database && npm run maintenance",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --incremental",
    "clean": "rm -rf .next node_modules/.cache",
    "install:clean": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "audit": "npm audit",
    "audit:fix": "npm audit fix --force",
    "upgrade": "npx npm-check-updates -u && npm install --legacy-peer-deps",
    "clean:all": "rm -rf node_modules .next package-lock.json && npm install --legacy-peer-deps"
  },
